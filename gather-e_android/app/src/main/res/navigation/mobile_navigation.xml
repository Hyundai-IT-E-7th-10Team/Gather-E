<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <include app:graph="@navigation/map" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.kosa.gather_e.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_list"
        android:name="com.kosa.gather_e.ui.chatlist.ChatListFragment"
        android:label="@string/title_list"
        tools:layout="@layout/fragment_chat_list" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.kosa.gather_e.ui.map.MapFragment"
        android:label="@string/title_map"
        tools:layout="@layout/activity_map" />

    <fragment
        android:id="@+id/navigation_mypage"
        android:name="com.kosa.gather_e.ui.mypage.MypageFragment"
        android:label="@string/title_mypage"
        tools:layout="@layout/fragment_mypage" />
</navigation>